    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "dotenv": "^16.4.5",
    "ai": "^5.0.68",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.9.5",
    "nanoid": "^5.1.6",
    "officeparser": "^5.2.1",
    "postgres": "^3.4.7",
    "pg": "^8.13.0",
    "zod": "^3.25.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
