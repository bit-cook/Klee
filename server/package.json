{
  "name": "klee-server",
  "version": "0.1.0",
  "exports": {
    "./client": {
      "types": "./src/index.ts"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
